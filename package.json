{
  "name": "react-generator-cmd",
  "version": "5.0.45",
  "description": "Generate redux or components",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js && cp cli.js dist",
    "g1": "node ./dist/main.js",
    "g": "node_modules/.bin/g",
    "release": "release-it --no-git.requireCleanWorkingDir"
  },
  "bin": {
    "g": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Michaeladze/react-generator.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "generate",
    "redux",
    "components"
  ],
  "author": "Michael Kutateladze",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Michaeladze/react-generator/issues"
  },
  "homepage": "https://github.com/Michaeladze/react-generator#readme",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.4.1",
    "@types/react-redux": "^7.1.23",
    "@types/redux-actions": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-loader": "^8.2.5",
    "eslint": "7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-unused-imports": "^2.0.0",
    "inquirer": "^7.3.3",
    "jest": "^27.5.1",
    "react-generator-cmd": "^5.0.43",
    "release-it": "^14.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "4.9.3",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "resolutions": {
    "eslint": "7.32.0"
  },
  "release-it": {
    "github": {
      "release": false
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "dependencies": {
    "rxjs": "^7.5.5"
  }
}
